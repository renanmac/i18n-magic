{
  "name": "i18n-magic",
  "displayName": "i18n Magic",
  "description": "Easily visualize i18n content in your vscode.",
  "version": "0.2.0",
  "publisher": "renanmacs",
  "main": "./extension.js",
  "icon": "assets/images/i18n-magic.png",
  "repository": {
    "type": "git",
    "url": "https://github.com/renanmac/i18n-magic"
  },
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "activationEvents": [
    "onLanguage:javascriptreact",
    "onLanguage:typescriptreact"
  ],
  "contributes": {
    "languages": [
      {
        "id": "javascriptreact",
        "extensions": [
          ".jsx"
        ]
      },
      {
        "id": "typescriptreact",
        "extensions": [
          ".tsx"
        ]
      }
    ],
    "grammars": [
      {
        "language": "javascript",
        "scopeName": "source.js.i18n",
        "path": "./syntaxes/javascriptreact.tmLanguage.json",
        "injectTo": [
          "source.js.jsx",
          "source.tsx"
        ]
      }
    ]
  }
}